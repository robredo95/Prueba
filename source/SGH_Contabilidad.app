.head 4 +  Actions
.head 5 -  Set sConsulta="select 
A.cod_cheque,
A.num_cheque,
convert(varchar(10),A.fecha,103) ,
A.monto,
A.beneficiario,
A.concepto,
C.nombre,
B.cuenta_banco,
A.cod_partida,
A.cod_conciliacion,
A.estado,
D.nombre

from cheques A,cuentas_banco B , bancos C, Tipo_Cheques D

where A.correl=B.correl and A.cod_banco=C.cod_banco 
      and A.cod_tipo=D.cod_tipo


"
			
.head 5 +  If cmbBanco!=''
.head 6 -  Set sConsulta = sConsulta ||  "  and A.cod_banco=:dlgConsultaCheques.cmbBanco.nCodigo "
.head 5 +  If cmbCuenta!=''
.head 6 -  Set sConsulta = sConsulta ||  "  and A.correl=:dlgConsultaCheques.cmbCuenta.nCodigo "
.head 5 +  If dfBeneficiario!=''
.head 6 -  Set sConsulta = sConsulta ||  "  and A.beneficiario like '%" || dlgConsultaCheques.dfBeneficiario || "%'"
.head 5 +  If dfConcepto!=''
.head 6 -  Set sConsulta = sConsulta ||  "  and A.concepto  like '%" || dlgConsultaCheques.dfConcepto || "%'"
.head 5 +  If dfFechaI != DATETIME_Null and dfFechaF !=DATETIME_Null
.head 6 -  Set sConsulta=sConsulta||"	AND	A.FECHA >= :dlgConsultaCheques.dfFechaI and
                                                A.FECHA <=:dlgConsultaCheques.dfFechaF "

.head 5 +  If cmbTipo!=''
.head 6 -  Set sConsulta = sConsulta ||  "  and A.cod_tipo=:dlgConsultaCheques.cmbTipo.nCodigo "
.head 5 +  If dfEstado!=''
.head 6 -  Set sConsulta = sConsulta ||  "  and A.estado like '" || dlgConsultaCheques.dfEstado || "%'"
.head 5 +  If dfConciliacion!=NUMBER_Null
.head 6 -  Set sConsulta = sConsulta ||  "  and A.cod_conciliacion=:dlgConsultaCheques.dfConciliacion "
.head 5 +  If dfChequeI !=NUMBER_Null and dfChequeF!=NUMBER_Null
.head 6 -  Set sConsulta = sConsulta ||  "  and A.num_cheque >=:dlgConsultaCheques.dfChequeI and 
A.num_cheque<=:dlgConsultaCheques.dfChequeF "
.head 5 +  If dfMontoI !=NUMBER_Null and dfMontoF!=NUMBER_Null
.head 6 -  Set sConsulta = sConsulta ||  "  and A.monto >=:dlgConsultaCheques.dfMontoI and 
A.monto<=:dlgConsultaCheques.dfMontoF "
.head 5 -  Set sConsulta=sConsulta||"	order	by	A.fecha"
.head 5 -  Return sConsulta
