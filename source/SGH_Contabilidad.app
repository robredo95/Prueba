.head 5 +  On SAM_Click
.head 6 -  Set sConsulta="	SELECT	A.cod_cheque,
			A.num_cheque,
			A.fecha,
			A.monto,
			A.beneficiario,
			A.concepto,
			C.nombre,
			B.cuenta_banco,
			D.nombre
		
		FROM cheques A,cuentas_banco B , bancos C, Tipo_Cheques D

		WHERE	A.correl=B.correl and A.cod_banco=C.cod_banco
			and A.cod_tipo=D.cod_tipo"
			
.head 6 +  If cmbBanco!=''
.head 7 -  Set sConsulta = sConsulta ||  "  and A.cod_banco=:dlgConsultaCheques.cmbBanco.nCodigo "
.head 6 +  If cmbCuenta!=''
.head 7 -  Set sConsulta = sConsulta ||  "  and A.correl=:dlgConsultaCheques.cmbCuenta.nCodigo "
.head 6 +  If dfBeneficiario!=''
.head 7 -  Set sConsulta = sConsulta ||  "  and A.beneficiario like '%" || dlgConsultaCheques.dfBeneficiario || "%'"
.head 6 +  If dfConcepto!=''
.head 7 -  Set sConsulta = sConsulta ||  "  and A.concepto  like '%" || dlgConsultaCheques.dfConcepto || "%'"
.head 6 +  If dfFechaI != DATETIME_Null and dfFechaF !=DATETIME_Null
.head 7 -  Set sConsulta=sConsulta||"	AND	A.FECHA >= :dlgConsultaCheques.dfFechaI and
                                                A.FECHA <=:dlgConsultaCheques.dfFechaF "

.head 6 +  If cmbTipo!=''
.head 7 -  Set sConsulta = sConsulta ||  "  and A.cod_tipo=:dlgConsultaCheques.cmbTipo.nCodigo "
.head 6 +  If dfEstado!=''
.head 7 -  Set sConsulta = sConsulta ||  "  and A.estado like '" || dlgConsultaCheques.dfEstado || "%'"
.head 6 +  If dfConciliacion!=NUMBER_Null
.head 7 -  Set sConsulta = sConsulta ||  "  and A.cod_conciliacion=:dlgConsultaCheques.dfConciliacion "
.head 6 +  If dfChequeI !=NUMBER_Null and dfChequeF!=NUMBER_Null
.head 7 -  Set sConsulta = sConsulta ||  "  and A.num_cheque >=:dlgConsultaCheques.dfChequeI and
A.num_cheque<=:dlgConsultaCheques.dfChequeF "
.head 6 +  If dfMontoI !=NUMBER_Null and dfMontoF!=NUMBER_Null
.head 7 -  Set sConsulta = sConsulta ||  "  and A.monto >=:dlgConsultaCheques.dfMontoI and
A.monto<=:dlgConsultaCheques.dfMontoF "
.head 6 -  Set sConsulta=sConsulta||"	order	by	A.fecha"
.head 6 -  Call SalCreateWindow( frmImprimir, hWndForm,
"Listado de Cheques",
strRepPath || "\\_Cheques.QRP",
":s1, :n1, :dt1, :n2, :s2, :s3, :s4, :s5, :s6",
"CodCheque, NumCheque, Fecha, Monto, Beneficiario, Concepto, Banco, Cuenta, TipoCheque",
sConsulta || " INTO :s1, :n1, :dt1, :n2, :s2, :s3, :s4, :s5, :s6", " ",FALSE)
