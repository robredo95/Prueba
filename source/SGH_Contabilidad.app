.head 4 -  Description:
.head 4 +  Returns
.head 5 -  String:
.head 4 -  Parameters
.head 4 -  Static Variables
.head 4 +  Local variables
.head 5 -  String: sConsulta
.head 4 +  Actions
.head 5 -  Set sConsulta="Select  distinct  A.COD_PARTIDA,   convert(varchar(12),A.FECHA,103),
		A.DESCRIPCION,C.NOMBRE,
		A.TOTAL_CARGO,A.TOTAL_DEBITO
			
		
		from	 PARTIDAS  A,DET_PARTIDAS  B,TIPOS_PARTIDA C

		where	A.COD_PARTIDA=B.COD_PARTIDA and A.COD_TIPO=C.COD_TIPO

"
			
.head 5 -  Set sConsulta2="select A.cod_partida,A.descripcion,A.fecha,B.cod_cuenta,
B.nombre,B.cargo,B.credito,B.concepto from partidas A,det_partidas B , TIPOS_PARTIDA C
where A.cod_partida=B.cod_partida and  A.COD_TIPO=C.COD_TIPO

"
			
.head 5 +  If dfDescripcion!=''
.head 6 -  Set sConsulta=sConsulta||"	AND	A.DESCRIPCION  like '%"||dlgPartidas.dfDescripcion||"%'"
.head 6 -  Set sConsulta2=sConsulta2||"	AND	A.DESCRIPCION  like '%"||dlgPartidas.dfDescripcion||"%'"
.head 5 +  If dfCuenta!=''
.head 6 -  Set sConsulta=sConsulta||"	AND	B.NOMBRE  like '%"||dlgPartidas.dfCuenta||"%'"
.head 6 -  Set sConsulta2=sConsulta2||"	AND	B.NOMBRE  like '%"||dlgPartidas.dfCuenta||"%'"
.head 5 +  If dfConcepto!=''
.head 6 -  Set sConsulta=sConsulta||"	AND	B.CONCEPTO  like '%"||dlgPartidas.dfConcepto||"%'"
.head 6 -  Set sConsulta2=sConsulta2||"	AND	B.CONCEPTO  like '%"||dlgPartidas.dfConcepto||"%'"
.head 5 +  If dfFechaI != DATETIME_Null and dfFechaF !=DATETIME_Null
.head 6 -  Set sConsulta=sConsulta||"	AND	A.FECHA >= :dlgPartidas.dfFechaI and
                                                A.FECHA <=  :dlgPartidas.dfFechaF "

.head 6 -  Set sConsulta2=sConsulta2||"	AND	A.FECHA >= :dlgPartidas.dfFechaI and
                                                A.FECHA <=  :dlgPartidas.dfFechaF "

.head 5 +  If cmbTipo!=''
.head 6 -  Set sConsulta=sConsulta||"	AND	A.COD_TIPO=:dlgPartidas.cmbTipo.nCodigo"
.head 6 -  Set sConsulta2=sConsulta2||"	AND	A.COD_TIPO=:dlgPartidas.cmbTipo.nCodigo"
.head 5 +  If cmbCuenta!=''
.head 6 -  Set sConsulta=sConsulta||"	AND	B.COD_CUENTA=:dlgPartidas.cmbCuenta.sCodigos"
.head 6 -  Set sConsulta2=sConsulta2||"	AND	B.COD_CUENTA=:dlgPartidas.cmbCuenta.sCodigos"
.head 5 -  Set sConsulta2=sConsulta2||"	order	by	A.COD_PARTIDA"
.head 5 -  Set sConsulta=sConsulta||"	order	by	A.COD_PARTIDA"
.head 5 -  Return sConsulta
