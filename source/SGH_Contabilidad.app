	hist_cuentas B
  WHERE A.cod_cuenta=B.cod_cuenta AND
	(B.saldo_anual <> 0 OR B.saldo_mes <> 0 OR B.saldo_anual+B.saldo_mes <> 0) AND
	B.fechaI>=:frmHistoricos.dfFechaInicio and B.fechaF <=:frmHistoricos.dfFechaFinal AND
	(B.cod_cuenta LIKE '4%' OR B.cod_cuenta LIKE '5%')

  ORDER BY cod_cuenta

  INTO

