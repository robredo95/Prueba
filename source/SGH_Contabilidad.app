.head 4 -  Local variables
.head 4 +  Actions
.head 5 -  Call SalTblPopulate(tbl2,hSql1,"
select
	A.COD_FACTURA,
	A.CORREL,	
	B.NOMBRE,
	A.FECHA,
	A.COSTO,
	A.MONTO_DEBE,	
	A.DESCRIPCION
               
from
	DET_HONORARIOS  A,
	MEDICOS B

where
	A.ESTADO_PAGO=1 AND
	A.COD_FACTURA=:dfFactura and
	A.COD_MEDICO=B.COD_MEDICO
order by
	A.COD_FACTURA,A.FECHA  ",TBL_FillAll)
