.head 5 -  Set bMTM_ColSelChanged = ( SalStrLeftX( spTxt, 17 ) = "MTM_ColSelChanged" )
.head 5 -  ! Debug
.head 5 +  ! If sMsgPrefix = "MTM" AND NOT ( bMTM_RowSelChanged OR bMTM_ColSelChanged )
.head 6 -  Return TRUE
.head 5 -  !
