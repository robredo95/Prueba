.head 7 -  Set r_nLeft = r_nLeft - m_nScrPosH
.head 7 -  !
.head 7 +  If ( m_nAlign & MIMG_ALIGN_VCENTER )
.head 8 -  Set r_nTop = SalNumberTruncate( SalNumberMax( nTop, ( ( nBottom - nTop ) - nHt ) / 2 ), 10, 0 )
.head 7 +  Else If ( m_nAlign & MIMG_ALIGN_BOTTOM )
